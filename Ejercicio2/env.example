KEY_NAME=                     # Nombre de la clave SSH para acceder a la EC2
AWS_REGION=                  # Región donde se crean los recursos (ej: us-east-1)
AWS_IMAGE_ID=                # ID de la AMI (imagen base para EC2, ej: Amazon Linux 2)
AWS_INSTANCE_TYPE=           # Tipo de instancia EC2 (ej: t2.micro)
AWS_S3_NAME=                 # Nombre del bucket S3 (si se usa)
AWS_EC2_NAME=                # Nombre lógico de la instancia EC2
AWS_SG_EC2_NAME=             # Nombre del Security Group para EC2 (acceso HTTP/SSH)
AWS_SG_DB_NAME=              # Nombre del Security Group para RDS (acceso MySQL)
SG_EC2_NAME=                 # Alias interno del SG EC2 usado en el script
SG_DB_NAME=                  # Alias interno del SG DB usado en el script
DB_IDENTIFIER=               # Identificador único de la instancia RDS
DB_INSTANCE_CLASS=           # Clase de instancia RDS (ej: db.t3.micro)
DB_ENGINE=                   # Motor de base de datos (ej: mysql)
DB_MASTER_USER_NAME=         # Usuario administrador de la base de datos
DB_MASTER_PASSWORD=          # Contraseña del usuario administrador
DB_APP_NAME=                 # Nombre de la base de datos que usará la aplicación

EC2_APP_USER=                # Usuario del sistema dentro de la EC2 (ej: ec2-user)
EC2_APP_GROUP=               # Grupo del sistema dentro de la EC2 (ej: ec2-user)
APP_USER=                    # Usuario que usa la aplicación (puede coincidir con EC2_APP_USER)
APP_DIR=                     # Carpeta donde se instala la app (ej: /var/www/html)
APP_PORT=                    # Puerto HTTP donde corre la app (ej: 80)
DB_PORT=                     # Puerto de la base de datos (ej: 3306)
DB_APP_USER=                 # Usuario que la app usa para conectarse a la base de datos
DB_APP_PASSWORD=             # Contraseña del usuario de la app para la base de datos

APP_ADMIN_PASSWORD=          # Contraseña del administrador web de la app
APP_ADMIN_USER=              # Usuario administrador web de la app

APP_REPO_URL=                # URL del repositorio que se clona en la EC2 (ej: GitHub)

# Access Key temporal del lab (se copia al iniciar)
AWS_ACCESS_KEY_ID=
# Secret Key temporal del lab
AWS_SECRET_ACCESS_KEY=
# Token temporal del lab     
AWS_SESSION_TOKEN=
